<h1>Listing members</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Party</th>
  </tr>

<% @members.each do |member| %>
  <tr>
    <td><%= link_to image_tag(member.photo) + h(member.name), member %></td>
    <td><%= link_to h(member.party), member %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New member', new_member_path %>
